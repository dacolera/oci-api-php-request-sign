{
  "name": "dacolera/oci-api-php-request-sign",
  "description": "This package generates proper HTTP headers to sign Oracle Cloud Infrastructure API requests",
  "keywords": [
    "oci",
    "api",
    "request",
    "signature"
  ],
  "homepage": "https://github.com/dacolera/oci-api-php-request-sign",
  "authors": [
    {
      "name": "Jurandir Dacol Junior",
      "email": "dacolera360@gmail.com"
    }
  ],
  "version": "1.2.3",
  "license": "MIT",
  "require": {
    "php": "^7.1 || ^7.3 || ^7.4 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
    "ext-openssl": "*"
  },
  "scripts": {
    "test": "php vendor/phpunit/phpunit/phpunit tests"
  },
  "autoload": {
    "psr-4": {
      "Dacolera\\OCI\\": "src/Dacolera/OCI"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dacolera\\Test\\": "tests/Dacolera/Test"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}
